{
  "name": "serverless-sub-routing",
  "version": "1.0.0",
  "description": "Serverless Sub Routing with DI",
  "main": "./src/index.ts",
  "author": "matthew2564",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf artifacts build dist",
    "format": "prettier . --write",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepare": "husky install",
    "setup": "sls dynamodb install",
    "start": "tsc && serverless offline start",
    "package": "node build-artifact.js && zip lambda.zip dist/lambda.js && mkdir -p artifacts && mv lambda.zip artifacts/lambda.zip"
  },
  "keywords": [
    "serverless",
    "typescript",
    "di",
    "express"
  ],
  "dependencies": {
    "@aws-lambda-powertools/commons": "^1.16.0",
    "@aws-lambda-powertools/tracer": "^1.16.0",
    "@aws-sdk/client-dynamodb": "^3.454.0",
    "@aws-sdk/credential-providers": "^3.454.0",
    "@aws-sdk/lib-dynamodb": "^3.454.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "reflect-metadata": "^0.2.1",
    "routing-controllers": "^0.10.4",
    "serverless-http": "^3.2.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "esbuild": "^0.19.11",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "rimraf": "^5.0.5",
    "serverless": "^3.37.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-dynamodb": "^0.2.47",
    "serverless-offline": "^13.3.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix",
    "*.{js,css,md,ts}": "npm run format"
  }
}
