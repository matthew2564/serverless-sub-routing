{
  "name": "serverless-sub-routing",
  "version": "1.1.0",
  "description": "Serverless Sub Routing with DI",
  "main": "src/index.ts",
  "author": "matthew2564",
  "friendlyName": "Serverless Sub Routing",
  "license": "ISC",
  "scripts": {
    "build": "npm run clean && ts-node compile-service.ts",
    "clean": "rimraf artifacts build dist",
    "format": "prettier . --write",
    "generate-openapi-spec": "ts-node documentation/generate-openapi-spec.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "package": "npm run build && mv dist/src/** dist/ && zip lambda.zip dist/index.js && mkdir -p artifacts && mv lambda.zip artifacts/lambda.zip",
    "prepare": "husky install",
    "setup": "sls dynamodb install",
    "start": "npm run build -- --source-map && SLS_DEBUG=* sls offline start",
    "start:debug": "npm run clean && tsc && SLS_DEBUG=* sls offline start",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "serverless",
    "typescript",
    "di",
    "express"
  ],
  "dependencies": {
    "@aws-lambda-powertools/commons": "^1.17.0",
    "@aws-lambda-powertools/logger": "^1.17.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "@dvsa/cvs-microservice-common": "^0.8.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "reflect-metadata": "^0.2.1",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0",
    "serverless-http": "^3.2.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "class-validator-jsonschema": "^5.0.0",
    "esbuild": "^0.19.11",
    "esbuild-plugin-typescript-decorators": "^0.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "rimraf": "^5.0.5",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-dynamodb": "^0.2.47",
    "serverless-offline": "^13.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix",
    "*.{js,css,md,ts}": "npm run format"
  }
}
